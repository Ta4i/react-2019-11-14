{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "standard",
    "standard-react"
  ],
  "rules": {
    "react/jsx-no-target-blank": 0,
    "react/jsx-handler-names": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-no-bind": 0,
    "operator-linebreak": 0,
    "react/jsx-closing-tag-location": 0,
    "react/prop-types": 0,
    "max-len": [
      "error",
      120
    ],
    "no-prototype-builtins": 0
  },
  "settings": {
    "react": {
      "version": "16.10"
    }
  }
}
